{
  "name": "capstone",
  "version": "1.0.0",
  "description": "Intelligent Trip Planner Application ",
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config ./api/server.js",
    "test": "nyc mocha -r dotenv/config api/test/routes.test.js --exit --timeout 10000",
    "dev": "nodemon -r dotenv/config ./api/server.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quangLinh9038/capstone.git"
  },
  "keywords": [
    "capstone"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quangLinh9038/capstone/issues"
  },
  "homepage": "https://github.com/quangLinh9038/capstone#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.0.3",
    "morgan": "^1.10.0",
    "neo4j-driver": "^4.3.1",
    "neode": "^0.4.7",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0",
    "should": "^13.2.3"
  },
  "type": "commonjs"
}
